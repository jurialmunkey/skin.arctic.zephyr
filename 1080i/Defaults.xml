        <textcolor>DialogForeground</textcolor>
        <focusedcolor>White100</focusedcolor>
        <disabledcolor>Black12</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>

    

    <default type="progress">
        <texturebg colordiffuse="White12">osd/progress-bg.png</texturebg>
        <lefttexture colordiffuse="Highlight">osd/progress-end.png</lefttexture>
        <midtexture colordiffuse="Highlight">osd/progress-end.png</midtexture>
        <righttexture colordiffuse="White70">osd/progress-end.png</righttexture>
        <overlaytexture>noop</overlaytexture>
    </default>

    <default type="slider">
        <width>360</width>
        <height>23</height>
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <texturesliderbar>other_textures/OSD/slider_bg_2.png</texturesliderbar>
        <textureslidernib border="2">other_textures/OSD/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/OSD/SliderNibFO.png</textureslidernibfocus>
    </default>
    
    <default type="sliderex">
        <height>66</height>
        <selectedcolor>Highlight2</selectedcolor>
        <sliderwidth>230</sliderwidth>
        <sliderheight>23</sliderheight>
        <texturesliderbar>other_textures/OSD/slider_bg.png</texturesliderbar>
        <textureslidernib border="2">other_textures/OSD/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/OSD/SliderNibFO.png</textureslidernibfocus>
        <font>Small</font>
        <aligny>center</aligny>
        <textcolor>DialogForeground</textcolor>
        <focusedcolor>White100</focusedcolor>
        <disabledcolor>Black12</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <textoffsety>0</textoffsety>
    </default>

    <default type="scrollbar">
        <width>8</width>
        <texturesliderbackground colordiffuse="White12">common/white.png</texturesliderbackground>
        <texturesliderbar colordiffuse="White30">common/white.png</texturesliderbar>
        <texturesliderbarfocus colordiffuse="Highlight">common/white.png</texturesliderbarfocus>
        <textureslidernib>noop</textureslidernib>
        <textureslidernibfocus>noop</textureslidernibfocus>
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>
    
    

    <default type="textbox">
        <font>Small</font>
        <textcolor>DialogForeground</textcolor>
        <shadowcolor>noop</shadowcolor>
        <selectedcolor>Highlight2</selectedcolor>
        <pagecontrol>61</pagecontrol>
        <autoscroll delay="6000" time="3000" repeat="10000"/>
    </default>

</includes>
